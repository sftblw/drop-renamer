{
  "build": {
    "beforeDevCommand": "npm run dev",
    "beforeBuildCommand": "npm run build",
    "devPath": "http://localhost:14213",
    "distDir": "../dist",
    "withGlobalTauri": false
  },
  "package": {
    "productName": "drop-renamer",
    "version": "0.0.1"
  },
  "tauri": {
    "allowlist": {
      "all": false,
      "shell": {
        "all": false,
        "open": true
      },
      "fs": {
        "renameFile": true,
        "removeFile": true,
        "createDir": true,
        "readDir": true,
        "readFile": true,
        "writeFile": true,
        "exists": true,
        "scope": ["$APPCONFIG", "$APPCONFIG/*", "/**/*"]
      },
      "path": {
        "all": true
      }
    },
    "bundle": {
      "active": true,
      "targets": "all",
      "identifier": "moe.sftblw.drop-renamer",
      "icon": [
        "icons/32x32.png",
        "icons/128x128.png",
        "icons/128x128@2x.png",
        "icons/icon.icns",
        "icons/icon.ico"
      ]
    },
    "security": {
      "csp": null
    },
    "windows": [
      {
        "fullscreen": false,
        "resizable": true,
        "title": "drop-renamer",
        "width": 800,
        "height": 600
      }
    ]
  }
}
